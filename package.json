{
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint --parallel",
    "tsc:check": "turbo run lint --parallel",
    "publish:patch": "yarn workspace @torolocos/react-rtc version --patch",
    "publish:minor": "yarn workspace @torolocos/react-rtc version --minor",
    "publish:major": "yarn workspace @torolocos/react-rtc version --major",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "turbo": "^1.2.16"
  },
  "packageManager": "yarn@1.22.17"
}
